{
  "name": "primereact-kit-tailwind",
  "version": "1.0.0",
  "description": "Tailwind CSS preset library for PrimeReact components using PassThrough API",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && npm run copy-types",
    "format": "prettier --write .",
    "copy-types": "cp src/index.d.ts dist/index.d.ts"
  },
  "keywords": [
    "primereact",
    "tailwind",
    "tailwindcss",
    "react",
    "ui",
    "components"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "primereact": "^10.0.0",
    "react": "^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "prettier": "^3.6.2",
    "primereact": "^10.9.7",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  }
}
